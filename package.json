{
	"name": "authoritarian",
	"version": "0.0.0-dev.27",
	"description": "node library for auth and user management",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"files": [
		"dist",
		"source",
		"generate-keys"
	],
	"scripts": {
		"prepare": "run-s clean build test",
		"clean": "rimraf dist && mkdirp dist dist/cjs",
		"build": "run-p build-esm build-cjs",
		"build-esm": "tsc",
		"build-cjs": "tsc --module commonjs --outDir dist/cjs",
		"watch": "tsc -w",
		"test": "./generate-keys && jest --verbose dist/cjs"
	},
	"devDependencies": {
		"@types/jest": "^24.0.18",
		"@types/jsonwebtoken": "^8.3.4",
		"barrelsby": "^2.1.1",
		"jest": "^24.9.0",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"replace": "^1.1.1",
		"rimraf": "^3.0.0",
		"typescript": "^3.6.3"
	},
	"dependencies": {
		"crosscall": "0.0.6-dev.11",
		"jsonwebtoken": "^8.5.1",
		"renraku": "0.0.0-dev.13"
	},
	"keywords": [
		"auth",
		"authentication",
		"authorization"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian/issues"
