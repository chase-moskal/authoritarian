{
	"name": "authoritarian",
	"version": "0.0.0-dev.9",
	"description": "node library for auth and user management",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"main": "dist",
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "run-s clean build test",
		"clean": "rm -rf dist && mkdir dist",
		"build": "run-s build-modules build-barrels",
		"build-modules": "tsc",
		"build-barrels": "barrelsby --directory source --exclude source/_archive --exclude source/main --delete",
		"watch": "tsc -w",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"keywords": [
		"auth",
		"authentication",
		"authorization"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian/issues"
	},
	"homepage": "https://github.com/chase-moskal/authoritarian#readme",
	"devDependencies": {
		"@types/express": "4.16.1",
		"@types/jsonwebtoken": "8.3.0",
		"@types/koa": "2.0.48",
		"@types/koa-router": "7.0.39",
		"barrelsby": "1.0.2",
		"npm-run-all": "4.1.5",
		"typescript": "3.3.1"
	},
	"dependencies": {
		"express": "4.16.4",
		"fancyfs": "0.0.0-dev.1",
		"google-auth-library": "3.0.1",
		"jsonwebtoken": "8.4.0",
		"koa": "2.7.0",
		"koa-router": "7.4.0"
	}
}
