{
	"private": true,
	"name": "authoritarian",
	"version": "0.0.0-dev.1",
	"description": "web authentication and authorization",
	"main": "dist",
	"scripts": {
		"docker": "docker-compose build && docker-compose up",
		"prepare": "run-s clean build-barrels build test",
		"build-barrels": "barrelsby --directory source --exclude source/_archive --exclude source/main --delete",
		"clean": "rm -rf dist && mkdir dist",
		"build": "tsc",
		"watch": "tsc -w",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"keywords": [
		"auth",
		"authentication",
		"authorization"
	],
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"devDependencies": {
		"@types/express": "4.16.0",
		"@types/jsonwebtoken": "8.3.0",
		"@types/koa": "2.0.47",
		"@types/koa-router": "7.0.35",
		"@types/node": "10.12.12",
		"barrelsby": "1.0.2",
		"npm-run-all": "4.1.5",
		"typescript": "3.2.1"
	},
	"dependencies": {
		"express": "4.16.4",
		"google-auth-library": "2.0.1",
		"jsonwebtoken": "8.4.0",
		"koa": "2.6.2",
		"koa-router": "7.4.0"
	}
}
