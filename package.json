{
	"name": "authoritarian",
	"version": "0.0.0-dev.12",
	"description": "node library for auth and user management",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"main": "dist/cjs/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "run-s clean build test",
		"clean": "rimraf dist && mkdirp dist dist/cjs",
		"build": "run-s build-barrels && run-p build-esm build-cjs",
		"build-barrels": "barrelsby --directory source --exclude source/_archive --exclude source/main --delete && replace --silent '\";$' '.js\"' source/index.ts",
		"build-esm": "tsc",
		"build-cjs": "tsc --module commonjs --outDir dist/cjs",
		"watch": "tsc -w",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"devDependencies": {
		"@types/jsonwebtoken": "^8.3.3",
		"barrelsby": "^2.1.1",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"replace": "^1.1.1",
		"rimraf": "^3.0.0",
		"typescript": "^3.6.2"
	},
	"dependencies": {
		"google-auth-library": "^5.2.0",
		"jsonwebtoken": "^8.5.1",
		"renraku": "0.0.0-dev.7"
	},
	"keywords": [
		"auth",
		"authentication",
		"authorization"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian/issues"
	},
	"homepage": "https://github.com/chase-moskal/authoritarian#readme"
}
