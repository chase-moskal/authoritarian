{
	"name": "authoritarian",
	"version": "0.0.0-dev.48",
	"description": "node library for auth and user management",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"type": "module",
	"files": [
		"dist",
		"source"
	],
	"scripts": {
		"prepare": "run-s clean && run-p importmap compile && run-s test",
		"clean": "rimraf dist *.pem && mkdirp dist",
		"importmap": "importly < package.json > dist/importmap.json",
		"compile": "tsc",
		"watch": "tsc -w",
		"start": "run-p testserve",
		"testserve1": "serve -l 8044",
		"testserve2": "serve -l 8045",
		"testserve": "run-p testserve1 testserve2",
		"test": "exit 0"
	},
	"devDependencies": {
		"@types/mongodb": "^3.5.4",
		"importly": "0.1.0-dev.0",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"serve": "^11.3.0",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"crosscall": "0.0.6-dev.19",
		"google-auth-library": "^6.0.0",
		"js-yaml": "^3.13.1",
		"mongodb": "^3.5.5",
		"redcrypto": "0.0.0-dev.5",
		"renraku": "0.0.0-dev.25"
	},
	"keywords": [
		"auth",
		"authentication",
		"authorization"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian/issues"
	},
	"homepage": "https://github.com/chase-moskal/authoritarian#readme"
}
