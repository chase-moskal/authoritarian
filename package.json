	"scripts": {
		"build": "run-s clean && run-p importmap compile && run-s test",
		"clean": "rimraf dist *.pem && mkdirp dist",
		"importmap": "importly < package.json > dist/importmap.json",
		"compile": "tsc",
		"watch": "tsc -w",
		"generate-keys": "./node_modules/redcrypto/generate-keys && mv private.pem metalback/config/auth-server.private.pem && mv public.pem metalback/config/auth-server.public.pem",
		"test": "exit 0"
	},
